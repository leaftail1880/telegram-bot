{
	"security.workspace.trust.untrustedFiles": "open",
	"editor.tabSize": 2,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.wordWrap": "on",
	"workbench.iconTheme": "material-icon-theme",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"explorer.confirmDragAndDrop": false,
	"terminal.integrated.fontFamily": "monospace",
	"editor.fontFamily": "Fira Code",
	"editor.fontLigatures": true,
	"debug.console.fontFamily": "Fira Code",
	"editor.inlayHints.fontFamily": "Fira Code",
	"launch": {
		"configurations": [],
		"compounds": []
	},
	"git.autofetch": true,
	"git.enableSmartCommit": true,
	"git.confirmSync": false,
	"javascript.preferences.importModuleSpecifier": "project-relative",
	"javascript.preferences.importModuleSpecifierEnding": "js",
	"workbench.colorTheme": "Atom One Dark",
	"files.associations": {
		"COMMIT_EDITMSG": "git-commit",
		"*.json": "jsonc"
	},
	"files.autoSave": "onFocusChange",
	"editor.stickyScroll.enabled": true,
	"editor.codeActionsOnSave": {
		"addMissingImports": true,
		"source.organizeImports": true
	},
	"javascript.referencesCodeLens.enabled": true,
	"commentTranslate.multiLineMerge": true,
	"commentTranslate.hover.content": false,
	"commentTranslate.ignore": [
		{},
		{
			"languageId": "javascript,typescript,javascriptreact,typescriptreact",
			"regular": "[\\*\\s]+"
		},
		{
			"languageId": "dart",
			"regular": "[\\s|/]+"
		}
	],
	"workbench.productIconTheme": "icons-carbon",
	"material-icon-theme.opacity": 1,
	"material-icon-theme.folders.theme": "specific",
	"material-icon-theme.saturation": 0.9,
	"material-icon-theme.folders.associations": {
		"lib": "Less",
		"modules": "Constant",
		"module": "Constant",
		"scripts": "Api"
	},
	"material-icon-theme.folders.color": "#ef5350",
	"material-icon-theme.hidesExplorerArrows": true,
	"editor.formatOnSave": true,
	"workbench.editor.closeOnFileDelete": true,
	"indentRainbow.indicatorStyle": "light",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"prettier.useTabs": true
}
